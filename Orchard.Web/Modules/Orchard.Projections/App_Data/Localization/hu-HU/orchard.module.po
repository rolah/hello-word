msgid "The maximum number of characters this property can be."
msgstr "A tulajdonság maximális hossza, karakterben."

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Trim only on a word boundary"
msgstr "Elválasztás csak szóhatáron"

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Checked to trim the property only on a word boundary."
msgstr "Jelölje be, hogy a tulajdonság csak szóhatáron legyen elválasztva."

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Add an Ellipsis"
msgstr "Szókihagyás jelzésének hozzáadása"

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Check to insert and ellipsis ({0}) if the property is trimmed."
msgstr "Jelölje be, hogy megjelenjen a szókihagyás jelzése ({0}), ha a tulajdonság rövidítve van."

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Strip HTML tags"
msgstr "HTML tagek eltávolítása"

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Check to remove any HTML tag from the property."
msgstr "Jelölje be, hogy minden HTML tag el legyen távolítva a tulajdonságból."

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Remove whitespace"
msgstr "Üres helyek (szóközök, tabulátorok...) eltávolítása"

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Check to remove the spaces at both the beginning and the end of the property."
msgstr "Jelölje be, hogy a tulajdonságok elején és végén található szóközök el legyenek távolítva."

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Preserve new lines"
msgstr "Sortörések megtartása"

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Check to convert a line break to a &lt;br /&gt; tag."
msgstr "Jelölje be, hogy a sortörések &lt;br /&gt; taggé legyenek alakítva."

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Save"
msgstr "Mentés"

#: "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgctxt "~/Modules/Orchard.Projections/Views/Property/Edit.cshtml"
msgid "Cancel"
msgstr "Mégse"

